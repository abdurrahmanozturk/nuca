{
  "scopeName": "source.fraptran",
  "name": "FRAPTRAN",
  "patterns": [
    { "include": "#comments" },
    { "include": "#blocks" },
    { "include": "#keywords" },
    { "include": "#numbers" },
    { "include": "#operators" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.star.fraptran",
          "match": "^\\*.*$"
        }
      ]
    },
    "blocks": {
      "patterns": [
        {
          "name": "keyword.control.block.fraptran",
          "match": "\\$(frtran|end)\\b"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.other.fraptran",
          "match": "\\b(tend|dt|npower|power|coolant|pressure)\\b"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.fraptran",
          "match": "\\b[0-9]+(\\.[0-9]+)?(E[+-]?[0-9]+)?\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.assignment.fraptran",
          "match": "="
        }
      ]
    }
  },
  "fileTypes": ["ftn", "frt", "inp"],
  "uuid": "4f3d3e90-fb90-11ee-8c99-0242ac120002"
}
